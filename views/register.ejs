<%- include('./partials/header') %>
<form class="form-container" action="/users/register" method="POST">
  <h1 class="container-title">SIGN UP</h1>
  <div class="name-input">
    <input type="name" name="name" value="" placeholder="Name">
  </div>
  <div class="email-input">
    <input type="email" name="email" value="" placeholder="Email">
  </div>
  <div id="password-input">
    <input type="password" name="password" value="" placeholder="Password">
  </div>
  <div id="confirm-password-input">
    <input type="password" name="confirm_password" value="" placeholder="Confirm password">
  </div>
  <button>Submit</button>
  <p>
    <a href="/users/auth/google">Google Sign In/Sign Up</a>
  </p>
</form>
<%- include('./partials/footer') %>
